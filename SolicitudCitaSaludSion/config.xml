<?xml version='1.0' encoding='utf-8'?>
<widget id="co.com.softcomputo.solicitudcitasaludsion" version="1.2.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
        <name>Solicitud de Citas Salud Sion</name>
    <description>
        Solicitud de citas salud sion.
    </description>
    <author email="jgomez.softcomputo@gmail.com" href="http://softcomputo.com.co">
        Jhonny Gomez
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <plugin name="cordova-plugin-device" source="npm" spec="~1.1.1" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" spec="~1.3.0" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-splashscreen" source="npm" spec="~3.2.1" />
    <plugin name="cordova-plugin-vibration" source="npm" spec="~2.1.0" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
    <platform name="android">
        <icon density="ldpi" src="www/res/icon/android/mipmap-ldpi/ic_launcher.png" />
        <icon density="mdpi" src="www/res/icon/android/mipmap-mdpi/ic_launcher.png" />
        <icon density="hdpi" src="www/res/icon/android/mipmap-hdpi/ic_launcher.png" />
        <icon density="xhdpi" src="www/res/icon/android/mipmap-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="www/res/icon/android/mipmap-xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="www/res/icon/android/mipmap-xxxhdpi/ic_launcher.png" />

        <splash density="land-ldpi" src="www/res/screen/android/drawable-ldpi/Picture.png" />
        <splash density="land-mdpi" src="www/res/screen/android/drawable-mdpi/Picture.png" />
        <splash density="land-hdpi" src="www/res/screen/android/drawable-hdpi/Picture.png" />
        <splash density="land-xhdpi" src="www/res/screen/android/drawable-xhdpi/Picture.png" />
        <splash density="land-xxhdpi" src="www/res/screen/android/drawable-xxhdpi/Picture.png" />
        <splash density="land-xxxhdpi" src="www/res/screen/android/drawable-xxxhdpi/Picture.png" />

        <splash density="port-ldpi" src="www/res/screen/android/drawable-ldpi/Picture.png" />
        <splash density="port-mdpi" src="www/res/screen/android/drawable-mdpi/Picture.png" />
        <splash density="port-hdpi" src="www/res/screen/android/drawable-hdpi/Picture.png" />
        <splash density="port-xhdpi" src="www/res/screen/android/drawable-xhdpi/Picture.png" />
        <splash density="port-xxhdpi" src="www/res/screen/android/drawable-xxhdpi/Picture.png" />
        <splash density="port-xxxhdpi" src="www/res/screen/android/drawable-xxxhdpi/Picture.png" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
</widget>
